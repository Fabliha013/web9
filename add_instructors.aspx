    <script type="text/javascript">
        function insert() {
            var name = document.getElementById("Text1").value;
            var roll = document.getElementById("Text2").value;
            var dept = document.getElementById("Text3").value;
            var batch = document.getElementById("Text4").value;
            var desi = document.getElementById("Text5").value;

            var xmlhttp = new XMLHttpRequest();
            xmlhttp.open("GET", "insert.aspx?na=" + name + "&ro=" + roll + "&dep=" + dept + "&ba=" + batch + "&de=" + desi+ "&opr=insert", false);
            xmlhttp.send(null);

            document.getElementById("Text1").value = "";
            document.getElementById("Text2").value = "";
            document.getElementById("Text3").value = "";
            document.getElementById("Text4").value = "";
            document.getElementById("Text5").value = "";
            display();

            alert("record inserted successfully");

        }
        function delete1() {
            var name = document.getElementById("Text1").value;
           

            var xmlhttp = new XMLHttpRequest();
            xmlhttp.open("GET", "insert.aspx?na=" + name +  "&opr=delete", false);
            xmlhttp.send(null);

            document.getElementById("Text1").value = "";
           
            display();

            alert("record deleted successfully");

        }
        function display() {
          

            var xmlhttp = new XMLHttpRequest();
            xmlhttp.open("GET", "insert.aspx?opr=display", false);
            xmlhttp.send(null);

            document.getElementById("d1").innerHTML = xmlhttp.responseText;

            

        }
        function search() {


            var xmlhttp = new XMLHttpRequest();
            xmlhttp.open("GET", "insert.aspx?opr=search&nm1="+document.getElementById("Text1").value, false);
            xmlhttp.send(null);

            document.getElementById("d1").innerHTML = xmlhttp.responseText;



        }
